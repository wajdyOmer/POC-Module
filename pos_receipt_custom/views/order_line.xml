<odoo>
    <data>
        <record id="customization_view_pos_order_line" model="ir.ui.view">
            <field name="name">pos.order.line.list</field>
            <field name="model">pos.order.line</field>
            <field name="arch" type="xml">
                <list string="POS Order lines" editable="bottom" >
                    <field name="product_id" readonly="1"/>
                    <field name='order_id' readonly="1"/>
                    <field name="qty" readonly="1" sum="Total qty"/>
                    <field name="discount" readonly="1"/>
                    <field name="price_unit" readonly="1" widget="monetary"/>
                    <field name="price_subtotal" readonly="1" sum="Sum of subtotals" widget="monetary"/>
                    <field name="price_subtotal_incl" readonly="1" sum="Sum of subtotals" widget="monetary"/>
                    <field name="create_date" readonly="1"/>
                    <field name="currency_id" column_invisible="True"/>
                    <field name="employee_ids" readonly="0" widget='many2many_tags' />
                </list>
            </field>
        </record>

        <record id="customization_action_pos_order_line" model="ir.actions.act_window">
            <field name="name">POS Order Lines</field>
            <field name="res_model">pos.order.line</field>
            <field name="view_mode">list</field>
            <field name="view_id" ref="customization_view_pos_order_line"/>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No POS order lines found.
                </p>
            </field>
        </record>

        <menuitem id="customization_menu_pos_order_line"
                  name="POS Order Lines (Employess Page)"
                  parent="point_of_sale.menu_point_of_sale"
                  action="customization_action_pos_order_line"
                  sequence="10"/>

    </data>
</odoo>