<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="pos_receipt_custom.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1" >
        <xpath expr="//div[contains(@class,'o_list_buttons')]" position="inside">
            <button class="btn btn-secondary" 
                style = "width: 55px;height: 40px;font-size: 10px;margin-left: 8px;"
                t-on-click.stop = 'this.ActionPastePlan'>
                <span><i class="fa fa-clipboard"></i> Export Order Data</span>
            </button>
        </xpath>
    </t>
</templates>